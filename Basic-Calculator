function add(num1 , num2 ){
    return num1 + num2;
}
function subtract(num1 , num2 ){
    return num2 - num1;
}

function multiply(num1 , num2 ){
    return num1 * num2;
}

function divide(num1 , num2 ){
    return num1 / num2;
}

function calculator(n1 , n2 , operator) {
    return operator(n1 , n2);
}
